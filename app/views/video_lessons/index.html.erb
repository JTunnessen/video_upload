<p id="notice"><%= notice %></p>

<h1>Listing Video Lessons</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Video</th>
      <th>Youtube</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @video_lessons.each do |video_lesson| %>
      <tr>
        <td><%= video_lesson.title %></td>
        <td><%= video_lesson.description %></td>
        <td><%= video_lesson.video %></td>
        <td><%= video_lesson.youtube %></td>
        <td><%= link_to 'Show', video_lesson %></td>
        <td><%= link_to 'Edit', edit_video_lesson_path(video_lesson) %></td>
        <td><%= link_to 'Destroy', video_lesson, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Video lesson', new_video_lesson_path, class: 'button' %>
